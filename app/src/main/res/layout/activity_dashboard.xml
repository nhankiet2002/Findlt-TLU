<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/r6w9myzt70p3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingVertical="20dp"
                    android:paddingHorizontal="25dp"
                    android:orientation="horizontal">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/avatar_image_view"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:padding="4dp"
                        android:layout_marginEnd="20dp"
                        app:shapeAppearanceOverlay="@style/ShapeAppearance.Material3.Corner.Full"
                        android:src="@drawable/image7"
                        android:scaleType="centerCrop" />

                    <LinearLayout
                        android:id="@+id/riufyej4q04"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingVertical="6dp"
                        android:layout_marginEnd="12dp"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/r2lqwxo221ad"
                            android:textColor="#1D1B20"
                            android:textSize="22sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Admin"
                            />
                    </LinearLayout>
                    <ImageView
                        android:id="@+id/btn_go_to_admin_profile"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        app:srcCompat="@drawable/ic_chevrons_right"
                        android:scaleType="fitXY"
                        android:clickable="true"
                        android:focusable="true"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingVertical="10dp"
                    android:layout_marginBottom="31dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/btn_logout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingVertical="15dp"
                        android:paddingHorizontal="13dp"
                        android:layout_marginBottom="60dp"
                        android:layout_marginHorizontal="16dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/rhael7ta1zeq"
                            app:shapeAppearanceOverlay="@style/cr5"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_chevron_one_right"
                            android:scaleType="fitXY"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <TextView
                            android:id="@+id/r05cibtqkl713"
                            android:textColor="#1E1E1E"
                            android:textSize="20sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Đăng xuất"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/ryeuv3a8uvw"
                            app:shapeAppearanceOverlay="@style/cr5"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_logout"
                            android:scaleType="fitXY"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/btn_user"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingVertical="15dp"
                        android:paddingHorizontal="12dp"
                        android:layout_marginBottom="60dp"
                        android:layout_marginHorizontal="15dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/rhn6jqe48lq6"
                            app:shapeAppearanceOverlay="@style/cr5"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_users"
                            android:scaleType="fitXY"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <TextView
                            android:id="@+id/r69bd3thk1m5"
                            android:textColor="#1E1E1E"
                            android:textSize="20sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Người dùng"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/rl69y2fdlec"
                            app:shapeAppearanceOverlay="@style/cr5"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_chevron_one_right"
                            android:scaleType="fitXY"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/btn_censor"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:background="@drawable/s383838sw1cr8bffffff"
                        android:paddingVertical="15dp"
                        android:paddingHorizontal="12dp"
                        android:layout_marginBottom="60dp"
                        android:layout_marginHorizontal="15dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/rbwfrcxvy6is"
                            app:shapeAppearanceOverlay="@style/cr8"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_shield"
                            android:scaleType="fitXY"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <TextView
                            android:id="@+id/roa2ymst5zv"
                            android:textColor="#1E1E1E"
                            android:textSize="20sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Kiểm duyệt"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/r7vcr2g8xb9g"
                            app:shapeAppearanceOverlay="@style/cr8"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_chevron_one_right"
                            android:scaleType="fitXY"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/btn_featured"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        android:layout_marginBottom="60dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingHorizontal="12dp"
                        android:paddingVertical="15dp">

                        <ImageView
                            android:id="@+id/rgsbbiinlahm"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:scaleType="fitXY"
                            app:shapeAppearanceOverlay="@style/cr5"
                            app:srcCompat="@drawable/ic_star" />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <TextView
                            android:id="@+id/rrdw7se39ldl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nổi bật"
                            android:textColor="#1E1E1E"
                            android:textSize="20sp" />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/ru3qe8mi1w6o"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:scaleType="fitXY"
                            app:shapeAppearanceOverlay="@style/cr5"
                            app:srcCompat="@drawable/ic_chevron_one_right" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/btn_category"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingVertical="15dp"
                        android:paddingHorizontal="12dp"
                        android:layout_marginHorizontal="15dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/r1e8ic3m5io3"
                            app:shapeAppearanceOverlay="@style/cr5"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_tag"
                            android:scaleType="fitXY"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <TextView
                            android:id="@+id/rs70okr42eb"
                            android:textColor="#1E1E1E"
                            android:textSize="20sp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Danh mục"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            />
                        <ImageView
                            android:id="@+id/rq2uq78ub3l"
                            app:shapeAppearanceOverlay="@style/cr5"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:srcCompat="@drawable/ic_chevron_one_right"
                            android:scaleType="fitXY"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ryukajblb70i"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/rvnfnx2m9ui"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:paddingVertical="8dp"
                        android:paddingHorizontal="69dp"
                        android:orientation="vertical"
                        android:clickable="true"
                        android:focusable="true">
                        <ImageView
                            android:id="@+id/rlyyqonama0i"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_marginBottom="8dp"
                            app:srcCompat="@drawable/ic_home1"
                            android:scaleType="fitXY"
                            />
                        <TextView
                            android:id="@+id/rrfnmmtlchxh"
                            android:textColor="#1E1E1E"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Trang chủ"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/rn7dlaghzcfb"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:paddingVertical="8dp"
                        android:paddingHorizontal="81dp"
                        android:orientation="vertical"
                        android:clickable="true"
                        android:focusable="true">
                        <ImageView
                            android:id="@+id/rvum14zthb1"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_marginBottom="8dp"
                            app:srcCompat="@drawable/ic_one_user"
                            android:scaleType="fitXY"
                            />
                        <TextView
                            android:id="@+id/rxl697s6ik8"
                            android:textColor="#1E1E1E"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Admin"
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
